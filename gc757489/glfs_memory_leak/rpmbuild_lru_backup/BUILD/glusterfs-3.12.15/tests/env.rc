prefix=/usr
exec_prefix=/usr
libdir=/usr/lib64

PATH=/usr/sbin:$PATH
export PATH

GLUSTERD_PIDFILEDIR=/var/run/gluster
export GLUSTERD_PIDFILEDIR

LD_LIBRARY_PATH=/usr/lib64:$LD_LIBRARY_PATH
export LD_LIBRARY_PATH

LIBRARY_PATH=/usr/lib64:$LIBRARY_PATH
export LIBRARY_PATH

CPATH=/usr/include:$CPATH
export CPATH

GLUSTERD_WORKDIR=/var/lib/glusterd
export GLUSTERD_WORKDIR

PKG_CONFIG_PATH=${libdir}/pkgconfig:$PKG_CONFIG_PATH
export PKG_CONFIG_PATH

PYTHONPATH=${prefix}/lib/python2.7/site-packages:$PYTHON_PATH
export PYTHONPATH

PYTHON=/usr/bin/python
export PYTHON

PYTHONPATH=${prefix}/lib/python2.7/site-packages:$PYTHON_PATH
export PYTHONPATH
